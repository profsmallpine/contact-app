<div class="alert alert-info" role="alert">
	<h3 class="strong-body">Phone Numbers <span class="label label-default"><a class="emphasis-button" href="/contacts/<%= @contact.id %>/phone_numbers/new">Add</a></span></h3>
	<% @contact.phone_numbers.each do |phone_number| %>
		<p><%= phone_number.number_type %>: <%= phone_number.number %> - <a href="/contacts/<%= @contact.id %>/phone_numbers/<%= phone_number.id %>/edit">Edit</a> | <a href="/contacts/<%= @contact.id %>/phone_numbers/<%= phone_number.id %>/destroy">Delete</a></p>
	<% end %>
</div>